<?xml version="1.0"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule id="cadastro_economico_navigation">

		<navigation-case>
			<from-action>#{cadastroEconomicoPortalBean.salvar}</from-action>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/app/UC0044CadastroEconomicoPortal/T0044C-cadastro-economico-portal-display.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case id="cadastro_economico">
			<from-outcome>cadastro_economico</from-outcome>
			<to-view-id>/app/UC0044CadastroEconomicoPortal/T0044C-cadastro-economico-portal-form.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case >
			<from-outcome>editar_dados_economico</from-outcome>
			<to-view-id>/app/UC0044CadastroEconomicoPortal/T0044C-cadastro-economico-edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
